device nrf51822
speed 1000
w4 4001e504 2
w4 4001e50c 1
w4 4001e514 1
sleep 100
r
w4 4001e504 1
speed 1000
erase
loadbin $PWD/bootloader_uicr_36000.bin 0x10001000
loadbin $PWD/SoftDevice/softdevice_main.bin 0x0
loadbin $PWD/bootloader.bin 0x36000
loadbin $PWD/app.bin 0x14000
loadbin $PWD/app.sha1 0x3f800
r
q
